<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:name="com.example.androidtest.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.androidtest.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.androidtest.First"
            android:exported="false"
            android:label="@string/app_name"
            android:parentActivityName="com.example.androidtest.Register" >
        </activity>
        <activity
            android:name="com.example.androidtest.Register"
            android:exported="false"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.androidtest.Count"
            android:exported="false"
            android:label="@string/app_name"
            android:parentActivityName="com.example.androidtest.Register" >
        </activity>
        <activity
            android:name="com.example.androidtest.Back"
            android:exported="false"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.androidtest.Sign"
            android:exported="false"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.androidtest.Account"
            android:exported="false"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.androidtest.Graph"
            android:exported="false"
            android:label="@string/app_name" >
        </activity>

        <service
            android:name="com.example.androidtest.ActivityList"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>